   "source": [
    "import pyinputplus as pyip\n",
    "\n",
    "while True:\n",
    "    score = pyip.inputNum(\"請輸入學生分數: \", min=0, max=100)\n",
    "    if score >= 90:\n",
    "        print(\"優\")\n",
    "    elif score >= 80:\n",
    "        print(\"甲\")\n",
    "    elif score >= 70:\n",
    "        print(\"乙\")\n",
    "    elif score >= 60:\n",
    "        print(\"丙\")\n",
    "    else:\n",
    "        print(\"丁\")"
   ]
  }
